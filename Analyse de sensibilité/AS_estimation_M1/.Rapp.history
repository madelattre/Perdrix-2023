CE <- (sqrt(2)+log(1+sqrt(2)))/3#
CV <- 2/3#
#
esp2DM1<-function(D,H,lambda){#
  return(1/(CE*lambda*1/(sqrt(D)-1)+H))#
}#
#
varmod2DM1<-function(D,H,lambda,delta){#
  return(1/delta*(CV-CE^2)*(lambda*1/(sqrt(D)-1))^2/(CE*lambda*1/(sqrt(D)-1)+H)^3)#
}
